{
  "groups": {
    "group:admins": ["sjanglab-admins@"],
    "group:researchers": ["sjanglab-researchers@"],
    "group:students": ["sjanglab-students@"]
  },
  "acls": [
    {
      "action": "accept",
      "src": ["group:admins"],
      "dst": ["*:*"]
    },
    {
      "action": "accept",
      "src": ["group:researchers", "group:students"],
      "proto": "tcp",
      "dst": ["*:80", "*:443"]
    },
    {
      "action": "accept",
      "src": ["autogroup:member"],
      "dst": ["autogroup:self:*"]
    }
  ],
  "ssh": []
}
